section.social#join
  .container
    .main-block.social__main-block.wow.bounceInUp(data-wow-delay="0.1s")
      h2.main-block__title Stay tuned for additional features and exciting updates
      p.main-block__text to make your contribution even more rewarding!
      ul.soc-list.social__soc-list
        li.soc-list__item
          a.soc-list__link(href=process.env.TWITTER target="_blank") twitter
            +icon('tw')
        li.soc-list__item
          a.soc-list__link(href=process.env.TELEGRAM target="_blank") telegram
            +icon('tg')
